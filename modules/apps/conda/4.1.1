proc ModulesHelp { } {
  global version modroot
  puts stderr "apps/conda/4.1.1 - sets the Environment for conda 4.1.1"
}

module-whatis   "Sets the Environment for using miniconda package manager"

# for Tcl script use only
set     topdir          $::env(HOME)/install/bin/miniconda3
set     version         4.1.1
set     sys             linux86
conflict apps/conda

prepend-path    PATH            $topdir/bin
